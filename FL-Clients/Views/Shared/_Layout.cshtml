    <script src="/assetsLay/js/scripts.bundle.js"></script>
    <!--end::Global Javascript Bundle-->
    <!--begin::Page Custom Javascript(used by this page)-->
    <script src="/assetsLay/js/custom/widgets.js"></script>

    <script src="/js/new-target.js"></script>



    @await RenderSectionAsync("Scripts", required: false)
